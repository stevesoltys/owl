<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="owlConfiguration" class="com.stevesoltys.owl.config.OwlConfiguration">
    </bean>

    <bean id="componentConfiguration" class="com.stevesoltys.owl.config.OwlComponentConfiguration">
    </bean>

    <bean id="componentTypeConfiguration" class="com.stevesoltys.owl.config.OwlComponentTypeConfiguration">
    </bean>

    <bean id="agentConfiguration" class="com.stevesoltys.owl.config.OwlAgentConfiguration">
    </bean>

    <bean id="agentRepository" class="com.stevesoltys.owl.repository.AgentRepository">
    </bean>

    <bean id="componentControllerRepository" class="com.stevesoltys.owl.repository.OwlComponentControllerRepository">
    </bean>

    <bean id="componentRepository" class="com.stevesoltys.owl.repository.OwlComponentRepository">
    </bean>

    <bean id="componentTypeRepository" class="com.stevesoltys.owl.repository.OwlComponentTypeRepository">
    </bean>

</beans>